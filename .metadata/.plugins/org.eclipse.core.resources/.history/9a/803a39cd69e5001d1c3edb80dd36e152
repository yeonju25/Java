package ex04.Generic;

class Abc {}
class A extends Abc {}
class B extends Abc {}
class C extends Abc {}

class Box<T extends Abc>{
	private T ob;

	public T getOb() {
		return ob;
	}

	public void setOb(T ob) {
		this.ob = ob;
	}
}

class Unboxer{
	public static <T extends Abc> T openBox(Box<T> box) {
		return box.getOb();
	}
}

public class GenericMethodBox {

	public static void main(String[] args) {
		Box<A> boxA = new Box<A>();
		Box<B> boxB = new Box<B>();
		Box<C> boxC = new Box<C>();
		Box<Integer> box = new Box<Integer>();
		
		
	}

}
