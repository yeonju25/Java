package ex10;

public class CalcExam {

	public static void main(String[] args) {

		Calc add = new Add();
		int result = add.calcuate(10, 10);
		System.out.println(result);
		
		Calc sub = new Sub();
		result = sub.calcuate(5, 10);
		System.out.println(result);
		System.out.println("-----------------------");
		
		
		Calc add1 = new Calc() {
			@Override
			public int calcuate(int x, int y) {
				return x+y;
			}
		};
		
		result = add1.calcuate(5, 2);
		System.out.println(result);
		
		
	}

}
