package ex04;

public class Exam01 {

	public static void main(String[] args) {
		
		try {
			findClass();
		}catch(Exception e) {
			e.printStackTrace();
		}
		
		System.out.println("프로그램 종료");
	}

	private static void findClass() {
		try {
			Class.forName("java.lang.String2");
		} catch (ClassNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
